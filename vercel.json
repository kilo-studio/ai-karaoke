{
  "rewrites": [
    {
      "source": "/api/lyrics",
      "destination": "/api/lyrics/route.js"
    }
  ],
  "headers": [
    {
      "source": "/api/lyrics",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET,POST,OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "Content-Type, x-model" }
      ]
    }
  ]
}